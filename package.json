{
	"name": "@magnus-strategy/react-ga4",
	"version": "2.1.0",
	"author": "Magnus Strategy Group S.L.",
	"description": "react-ga4 parcel",
	"keywords": [],
	"homepage": "https://github.com/magnus-strategy/react-ga4",
	"bugs": {
		"url": "https://github.com/magnus-strategy/react-ga4/issues"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:magnus-strategy/react-ga4.git"
	},
	"license": "PRIVATE",
	"files": [
		"dist"
	],
	"type": "module",
	"module": "./dist/magnus-strategy-react-ga4.min.mjs",
	"umd:main": "./dist/magnus-strategy-react-ga4.umd.min.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/magnus-strategy-react-ga4.min.mjs"
		}
	},
	"scripts": {
		"build": "vite build",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"registry:login": "artifactregistry-auth --repo-config=./.npmrc --credential-config=./.npmrc && git update-index --assume-unchanged ./.npmrc",
		"postpublish": "gcsdeploy",
		"prepare": "vite build",
		"publish": "changeset publish",
		"typecheck": "tsc --noEmit",
		"version": "changeset version && yarn install --mode update-lockfile"
	},
	"devDependencies": {
		"@changesets/cli": "2.26.2",
		"@hexatool/eslint-config": "2.1.3",
		"@hexatool/tsconfig": "20.0.1",
		"@magnus-strategy/gcsdeploy": "1.0.2",
		"@magnus-strategy/vite-plugin-format-filename": "0.0.3",
		"@magnus-strategy/vite-plugin-format-globals": "0.0.3",
		"eslint": "8.50.0",
		"google-artifactregistry-auth": "3.1.2",
		"happy-dom": "12.9.0",
		"typescript": "5.2.2",
		"vite": "4.4.11",
		"vite-plugin-dts": "3.6.0",
		"vitest": "0.34.6"
	},
	"volta": {
		"node": "20.6.1",
		"yarn": "3.6.4"
	},
	"packageManager": "yarn@3.6.4",
	"gcsdeploy": {
		"project": "magnus-strategy",
		"bucket": "magnus-strategy-group-npm",
		"fileFilter": ".*\\.umd\\.min\\.js$",
		"cacheControl": "public, max-age=2592000, immutable"
	}
}
