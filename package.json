{
	"name": "@hexatool/google-analytics",
	"version": "0.0.0",
	"author": "Magnus Strategy Group S.L.",
	"description": "Google Analytics 4 library",
	"keywords": [
		"analytics",
		"google",
		"google analytics",
		"google analytics 4",
		"universal analytics"
	],
	"homepage": "https://github.com/hexatool/google-analytics",
	"bugs": {
		"url": "https://github.com/hexatool/google-analytics/issues"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:hexatool/google-analytics.git"
	},
	"license": "MIT",
	"files": [
		"dist"
	],
	"type": "module",
	"module": "./dist/google-analytics.min.mjs",
	"umd:main": "./dist/google-analytics.umd.min.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/google-analytics.min.mjs",
			"types": "./dist/index.d.ts"
		},
		"./react": {
			"import": "./dist/google-analytics-react.min.mjs",
			"types": "./dist/react.d.ts"
		}
	},
	"scripts": {
		"build": "vite build && vite build --config vite.config.react.ts",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"prepare": "vite build && vite build --config vite.config.react.ts",
		"publish": "changeset publish",
		"typecheck": "tsc --noEmit",
		"version": "changeset version && yarn install --mode update-lockfile"
	},
	"peerDependencies": {
		"react": ">=16.8.0"
	},
	"devDependencies": {
		"@changesets/cli": "2.26.2",
		"@hexatool/eslint-config": "2.1.3",
		"@hexatool/tsconfig": "20.0.1",
		"@types/react": "18.2.28",
		"eslint": "8.50.0",
		"happy-dom": "12.9.0",
		"react": "18.2.0",
		"typescript": "5.2.2",
		"vite": "4.4.11",
		"vite-plugin-dts": "3.6.0",
		"vitest": "0.34.6"
	},
	"volta": {
		"node": "20.6.1",
		"yarn": "3.6.4"
	},
	"packageManager": "yarn@3.6.4"
